<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bicepgamers">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.BicepGamers">
        <activity
            android:name=".GUI.HomePage"
            android:exported="true"
            android:label="@string/Games">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".GUI.ShowAllGames"
            android:exported="true"
            android:label="@string/Games"></activity>
        <activity
            android:name=".GUI.AddNewGame"
            android:exported="true"></activity>
        <activity
            android:name=".GUI.ShowAllSessions"
            android:exported="true"
            android:label="@string/History"></activity>
        <activity
            android:name=".GUI.SessionTimer"
            android:exported="true"
            android:label="@string/Timer"></activity>
        <activity android:name=".GUI.ExerciseActivity"
            android:exported="true"
            ></activity>
    </application>

</manifest>